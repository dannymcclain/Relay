{
  "name": "Relay",
  "manifest_version": 3,
  "description": "Show tabs from your synced devices",
  "version": "0.0.1",
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png",
    "128": "icons/icon-128.png",
    "256": "icons/icon-256.png"
  },
  "permissions": ["sessions", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["toggleIcon.js"]

    }
  ],
  "action": {
    "default_popup": "index.html"
  },
  "author": "Danny McClain <dannyxmcclain@gmail.com> (https://twitter.com/dannymcclain)"
}